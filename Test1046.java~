import java.util.*;

public class Test1046{
  public static void main(String[] args){
    
    Scanner sc = new Scanner(System.in);
    
    int x=sc.nextInt();
    int y=sc.nextInt();
    
    if(x==0 && y==0){
      System.out.println("O JOGO DUROU 24 HORA(S)");
    }else if(x>=0 && y<=23 && y>x){
      int p=y-x;
      System.out.println("O JOGO DUROU "+p+" HORA(S)");
    }else if(x>=0 || x<=23 && y>=0 || y<=23 && x>y){
      int q=x-12;
    }
  }
}