import java.util.Scanner;

public class Task23{
  public static void main(String[] args){
    
    Scanner sc = new Scanner (System.in);
    
    System.out.println("Enter number:");
    int x = sc.nextInt();
    
    for (int b=1; b<=x; b++){
      System.out.print(b);
    }
    
    for (int c=x-1; c>0; c--){
      System.out.print(c);
    }
  }
}
