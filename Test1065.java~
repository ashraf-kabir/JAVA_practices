import java.util.Scanner;

public class Test1065{
  public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    
    int num[]= new int[5];
    
    for(int a=1; a<num.length; a++){
      num[a]=sc.nextInt();
      int d=0;
      if(num[a] %2==0){
        d++;
      }
      System.out.println(d);
    }
  }
}