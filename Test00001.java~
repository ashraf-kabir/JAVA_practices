import java.io.IOException;

public class Test00001 {
 
    public static void main(String[] args) throws IOException {
        
        Scanner sc=new Scanner(System.in);
        
        System.out.println("Enter a number:");
        int A=sc.nextInt();
        
        System.out.println("Enter a number:");
        int B=sc.nextInt();
        
        int X=A+B;
        
        System.out.println("X = "+X);
    }
 
}