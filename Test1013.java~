import java.util.Scanner;

public class Test1013{
  public static void main(String argsg[]){
    Scanner sc=new Scanner(System.in);
    
    int a=sc.nextInt();
    int b=sc.nextInt();
    int c=sc.nextInt();
    
    //int ab=(a+b+(Math.abs(a))-(Math.abs(b)))/2;
    int x=(Math.abs(a))*(Math.abs(b));
    int abc=(a*b+c+(x-Math.abs(c)))/2;
    
    System.out.println(abc+" eh o maior");
  }
}
