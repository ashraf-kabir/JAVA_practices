public class Quiz8b{
  public static void main(String [] args){
    int i = 0;
    int j = 1;
    String [ ][ ] twoD1  = new String [3][3];
    int [ ][ ] twoD2  = new int [3][3];
    twoD1[0][0] = "s";
    twoD1[0][1] = "d";
    twoD1[0][2] = "x";
    twoD1[1][0] = "s";
    twoD1[1][1] = "b";
    twoD1[1][2] = "m";
    twoD1[2][0] = "z";
    twoD1[2][1] = "q";
    twoD1[2][2] = "t";
    twoD2[0][0] = 12;
    twoD2[0][1] = 52;
    twoD2[0][2] = 39;
    twoD2[1][0] = 11;
    twoD2[1][1] = 10;
    twoD2[1][2] = 90;
    twoD2[2][0] = 21;
    twoD2[2][1] = 2;
    twoD2[2][2] = 4;
    while (i < 3){
      j = 2;
      while (j >= 0){
        System.out.println(twoD1[i][j] + twoD2[j][i]);
        j--;
      }      
      ++i;
    }
  }
}
